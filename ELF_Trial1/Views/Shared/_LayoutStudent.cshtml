    <script type="text/javascript">
        function Logout() {
            $.ajax({
                type: 'POST',
                url: '/Student/Logout',
                contentType: 'application/json;charset=utf-8',
                data: JSON.stringify(),
                datatype: 'json',
                success: function (data) {
                    if (data == 'success') {
                        window.location.href = '/ELFWeb/index';
                    }
                },
            });
        }
